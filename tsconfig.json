{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@images/*": ["assets/images/*"],
      "@icons/*": ["./assets/icons/*"],
      "@api/*": ["src/api/*"],
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@navigators": ["src/navigators"],
      "@screens/*": ["src/screens/*"],
      "@utils": ["src/utils"],
      "@hooks": ["src/hooks"],
      "@interfaces": ["src/utils/interfaces.ts"]
    }
  }
}
